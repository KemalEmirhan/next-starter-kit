  "lint-staged": {
    "*.{js, jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "next": "11.1.0",
